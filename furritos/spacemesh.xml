<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <TemplateURL>https://raw.githubusercontent.com/furritos/docker-templates/master/furritos/smapp.xml</TemplateURL>
  <Beta>True</Beta>
  <Category>Crypto:</Category>
  <Date>2022-04-11</Date>
  <Changes>
    [center][font size=4]SMAPP Change Log[/font][/center]
    
    [font size=3]2022.04.11[/font]
    - Initial release
  </Changes>
  <Project>https://spacemesh.io/</Project>
  <Name>Spacemesh</Name>
  <Description>This project takes the standalone Spacemesh + Wallet application and transforms into portable, web-accessible container using Docker and noVNC.</Description>
  <Overview>This project takes the standalone Spacemesh + Wallet application and transforms into portable, web-accessible container using Docker and noVNC.</Overview>
  <Support>https://github.com/furritos/docker-smapp</Support>
  <Registry>https://hub.docker.com/r/furritos/docker-smapp:latest</Registry>
  <GitHub>https://github.com/furritos/docker-smapp</GitHub>
  <Repository>furritos/docker-smapp:latest</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>5801</HostPort>
        <ContainerPort>8080</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/smapp/config</HostDir>
      <ContainerDir>/root/.config/Spacemesh</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/Media</HostDir>
      <ContainerDir>/root/post</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:5801]</WebUI>
  <Banner></Banner>
  <Icon>https://github.com/furritos/docker-templates/raw/master/furritos/images/smapp.png</Icon>
  <ExtraParams></ExtraParams>
</Containers> 